<!--pages/login/login.wxml-->
<view class="contain">
	<view class="con">
		<view class="login">
			登录/绑定
		</view>
		<view class="login_1">欢迎登陆普华云服务</view>
	</view>
	<view class="tables">
		<view class="console">
			<van-cell-group>
				<van-field value="{{ username }}" required clearable placeholder="请输入用户名" left-icon='manager-o' />

				<van-field value="{{ password }}" type="password" placeholder="请输入密码" left-icon='records' required />
			</van-cell-group>
		</view>

	</view>
	<view class="btn">
		<view class="cancel" bindtap="goPersonal">
			取消
		</view>
    <view style="width:30px"></view>
		<button class="submit">登录</button>
	</view>
</view>